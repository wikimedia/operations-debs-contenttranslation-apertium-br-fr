    <rule comment="REGLA: NOM NUM A NOM">
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="num"/>
        <pattern-item n="a"/>
        <pattern-item n="nom"/>
      </pattern>
      <action>
        <call-macro n="f_concord1">
